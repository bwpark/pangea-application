        <addForeignKeyConstraint baseColumnNames="me_id"
                                 baseTableName="repute"
                                 constraintName="fk_repute_me_id"
                                 referencedColumnNames="id"
                                 referencedTableName="avatar"/>

    </changeSet>
</databaseChangeLog>
